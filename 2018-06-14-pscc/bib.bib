@article{fliscounakis2013,
	title = {Contingency {Ranking} {With} {Respect} to {Overloads} in {Very} {Large} {Power} {Systems} {Taking} {Into} {Account} {Uncertainty}, {Preventive}, and {Corrective} {Actions}},
