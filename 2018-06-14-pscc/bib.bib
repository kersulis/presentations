	keywords = {Mathematical model, load flow, optimization methods, Optimization, optimal power flow, Computational modeling, Programming, quadratic programming, convex programming, semidefinite programming, convex relaxation, Load flow, Convex quadratic optimization, polar representation, power flow equations, QC relaxation, quadratic convex relaxation, SDP, second-order cone relaxations, System-on-chip},
	pages = {3008--3018},
}

@article{grone1984,
